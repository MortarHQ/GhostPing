{
  "name": "ghostping",
  "version": "0.0.3",
  "private": false,
  "type": "module",
  "packageManager": "pnpm@9.0.0",
  "scripts": {
    "start": "pnpm run web:build && node scripts/run.mjs --env production src/appServer.ts",
    "dev": "node scripts/run.mjs --env development --watch src/appServer.ts",
    "web:dev": "vite --config frontend/vite.config.ts",
    "web:build": "vite build --config frontend/vite.config.ts",
    "web:preview": "vite preview --config frontend/vite.config.ts",
    "mc:ping": "node scripts/run.mjs --env test src/mcClient.ts",
    "test": "pnpm run mc:ping",
    "release:manifest": "node .github/scripts/generate-version-manifest.mjs",
    "release:manifest:check": "node .github/scripts/generate-version-manifest.mjs --check"
  },
  "dependencies": {
    "@monaco-editor/react": "^4.7.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dayjs": "^1.11.19",
    "js-beautify": "^1.15.4",
    "monaco-editor": "^0.52.2",
    "pino": "^10.3.0",
    "pino-pretty": "^13.1.3",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "tailwind-merge": "^3.3.1",
    "toml": "^3.0.0",
    "varint": "^6.0.0",
    "lucide-react": "^0.542.0"
  },
  "devDependencies": {
    "@tailwindcss/vite": "^4.1.13",
    "@types/react": "^19.1.12",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.1",
    "@types/js-beautify": "^1.14.3",
    "@types/node": "^25.2.0",
    "@types/varint": "^6.0.3",
    "tailwindcss": "^4.1.13",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vite": "^7.1.5"
  }
}
